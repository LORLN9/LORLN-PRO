-- ========== Ø¯Ø§Ù„Ø© Ø¥ØºÙ„Ø§Ù‚ Ø§Ù„Ù‚Ø§Ø¦Ù…Ø© ==========
local function closeMainWindow()
    if not main or isWindowClosed then return end
    
    lastWindowPosition = main.Position
    saveData("LastWindowPos", lastWindowPosition)
    
    isMinimized = false
    isWindowClosed = true
    
    if scroll then
        scroll.Visible = false
    end
    
    TS:Create(main, TweenInfo.new(0.25), {
        Size = UDim2.new(0, 0, 0, 0)
    }):Play()
    
    task.wait(0.25)
    main.Visible = false
end

