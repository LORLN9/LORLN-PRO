    autoBtn.Position = UDim2.new(1, -45, 0, 50)
    autoBtn.BackgroundColor3 = Color3.fromRGB(40, 0, 0)
    autoBtn.Image = "rbxassetid://131690304470847" -- Ø´Ø¹Ø§Ø± LORLN
    autoBtn.ImageColor3 = Theme.TextSecondary
    autoBtn.Parent = card
    
