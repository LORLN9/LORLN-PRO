            executeScript(code, name)
        end)
    end

    -- ÙˆØ¸ÙŠÙØ© Ø§Ù„Ø£ÙˆØªÙˆ
    local function toggleAuto()
        if autoScripts[name] == true then
            autoScripts[name] = false
            autoBtn.ImageColor3 = Theme.TextSecondary
            autoStroke.Color = Color3.fromRGB(100, 0, 0)
            autoLabel.TextColor3 = Theme.TextSecondary
            
            showNotification("AUTO MODE", "Auto-execute disabled", name, "Manual mode")
        else
            autoScripts[name] = true
            autoBtn.ImageColor3 = Theme.Success
            autoStroke.Color = Theme.Success
            autoLabel.TextColor3 = Theme.Success
            
            -- ØªØ´ØºÙŠÙ„ Ø§Ù„Ø³ÙƒØ±Ø¨Øª ÙÙˆØ±Ø§Ù‹
            local success = executeScript(code, name)
            if success then
                showNotification("AUTO MODE", "Auto-execute enabled", name, "Will auto-run on join")
            end
        end
        saveData("AutoScripts", autoScripts) -- Ø­ÙØ¸ Ø§Ù„Ø¨ÙŠØ§Ù†Ø§Øª
    end

    autoBtn.MouseButton1Click:Connect(toggleAuto)

