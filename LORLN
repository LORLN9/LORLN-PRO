local notifications = {}
local function showNotification(title, text, scriptName, placeName)
    local notificationFrame = Instance.new("Frame")
    notificationFrame.Name = "Notification"
    notificationFrame.Size = UDim2.new(0, 300, 0, 80)
    notificationFrame.Position = UDim2.new(1, -320, 0, 10 + (#notifications * 90))
    notificationFrame.BackgroundColor3 = Theme.Main
    notificationFrame.BackgroundTransparency = 0.1
    notificationFrame.Parent = sg
    
    local notifCorner = Instance.new("UICorner")
    notifCorner.CornerRadius = UDim.new(0, 10)
    notifCorner.Parent = notificationFrame
    
    local notifStroke = Instance.new("UIStroke")
    notifStroke.Color = Theme.Accent
    notifStroke.Thickness = 2
    notifStroke.Parent = notificationFrame
    
    -- Ø§Ù„Ø´Ø¹Ø§Ø±
    local logo = Instance.new("ImageLabel")
    logo.Size = UDim2.new(0, 40, 0, 40)
    logo.Position = UDim2.new(0, 10, 0.5, -20)
    logo.Image = "rbxassetid://131690304470847"
    logo.BackgroundTransparency = 1
    logo.Parent = notificationFrame
    
    -- Ø§Ù„Ø¹Ù†ÙˆØ§Ù†
    local titleLabel = Instance.new("TextLabel")
    titleLabel.Size = UDim2.new(1, -60, 0, 25)
    titleLabel.Position = UDim2.new(0, 60, 0, 10)
    titleLabel.Text = "âœ§ " .. title .. " âœ§"
    titleLabel.TextColor3 = Theme.Accent
    titleLabel.Font = Enum.Font.GothamBold
    titleLabel.TextSize = 14
    titleLabel.BackgroundTransparency = 1
    titleLabel.Parent = notificationFrame
    
    -- Ø§Ù„Ù†Øµ
    local textLabel = Instance.new("TextLabel")
    textLabel.Size = UDim2.new(1, -60, 0, 45)
    textLabel.Position = UDim2.new(0, 60, 0, 30)
    textLabel.Text = text .. "\n" .. scriptName .. " in " .. placeName
    textLabel.TextColor3 = Theme.Text
    textLabel.Font = Enum.Font.Gotham
    textLabel.TextSize = 11
    textLabel.TextWrapped = true
    textLabel.BackgroundTransparency = 1
    textLabel.Parent = notificationFrame
    
    table.insert(notifications, notificationFrame)
    
    -- Ø¥Ø®ÙØ§Ø¡ Ø§Ù„Ø¥Ø´Ø¹Ø§Ø± Ø¨Ø¹Ø¯ 5 Ø«ÙˆØ§Ù†ÙŠ
    task.wait(5)
    TS:Create(notificationFrame, TweenInfo.new(0.5), {
        Position = UDim2.new(1, 300, 0, notificationFrame.Position.Y.Offset)
    }):Play()
    task.wait(0.5)
    notificationFrame:Destroy()
    table.remove(notifications, 1)
    
    -- ØªØ­Ø¯ÙŠØ« Ù…ÙˆØ§Ù‚Ø¹ Ø§Ù„Ø¥Ø´Ø¹Ø§Ø±Ø§Øª Ø§Ù„Ù…ØªØ¨Ù‚ÙŠØ©
    for i, notif in ipairs(notifications) do
        TS:Create(notif, TweenInfo.new(0.3), {
            Position = UDim2.new(1, -320, 0, 10 + ((i-1) * 90))
        }):Play()
    end
end

