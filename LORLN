local keyDragging = false
local keyDragStart, keyStartPos

keyHeader.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        keyDragging = true
        keyDragStart = input.Position
        keyStartPos = keyWindow.Position
        keyStroke.Color = Color3.fromRGB(255, 100, 100)
    end
end)

keyHeader.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        keyDragging = false
        keyStroke.Color = Theme.Accent
    end
end)

UIS.InputChanged:Connect(function(input)
    if keyDragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
        local delta = input.Position - keyDragStart
        keyWindow.Position = UDim2.new(
            keyStartPos.X.Scale,
            keyStartPos.X.Offset + delta.X,
            keyStartPos.Y.Scale,
            keyStartPos.Y.Offset + delta.Y
        )
    end
end)

-- ØªØ£Ø«ÙŠØ±Ø§Øª Hover
