local function checkBannedPlayers()
    for _, player in ipairs(Players:GetPlayers()) do
        if bannedPlayers[player.Name] then
            -- Ø¥Ø¸Ù‡Ø§Ø± Ø±Ø³Ø§Ù„Ø© Ø§Ù„Ø¨Ø§Ù†Ø¯ Ù„Ù„Ø§Ø¹Ø¨ Ø§Ù„Ù…Ø¨Ù†Ø¯
            local banMessage = Instance.new("ScreenGui")
            banMessage.Name = "BanWarning"
            banMessage.Parent = player:WaitForChild("PlayerGui")
            
            local frame = Instance.new("Frame")
            frame.Size = UDim2.new(1, 0, 1, 0)
            frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            frame.BackgroundTransparency = 0.5
            frame.Parent = banMessage
            
            local label = Instance.new("TextLabel")
            label.Size = UDim2.new(0, 500, 0, 150)
            label.Position = UDim2.new(0.5, -250, 0.5, -75)
            label.Text = "ğŸš« ACCESS DENIED ğŸš«\n\nYou have been banned from LORLN PRO\n\nBanned by: " .. bannedPlayers[player.Name].bannedBy .. "\nDate: " .. bannedPlayers[player.Name].banDate .. "\nReason: " .. bannedPlayers[player.Name].reason
            label.TextColor3 = Color3.fromRGB(255, 50, 50)
            label.Font = Enum.Font.GothamBlack
            label.TextSize = 16
            label.TextWrapped = true
            label.BackgroundColor3 = Color3.fromRGB(20, 0, 0)
            label.Parent = frame
            
            local corner = Instance.new("UICorner")
            corner.CornerRadius = UDim.new(0, 15)
            corner.Parent = label
            
            local stroke = Instance.new("UIStroke")
            stroke.Color = Color3.fromRGB(255, 50, 50)
            stroke.Thickness = 4
            stroke.Parent = label
            
            -- Ø²Ø± Ø§Ù„Ø¥ØºÙ„Ø§Ù‚ (Ù„Ø§ ÙŠØ¹Ù…Ù„)
            local closeBtn = Instance.new("TextButton")
            closeBtn.Size = UDim2.new(0, 100, 0, 40)
            closeBtn.Position = UDim2.new(0.5, -50, 0.8, -20)
            closeBtn.Text = "CLOSE (Disabled)"
            closeBtn.TextColor3 = Color3.fromRGB(150, 150, 150)
            closeBtn.Font = Enum.Font.GothamBold
            closeBtn.TextSize = 14
            closeBtn.BackgroundColor3 = Color3.fromRGB(50, 0, 0)
            closeBtn.Parent = label
            
            local btnCorner = Instance.new("UICorner")
            btnCorner.CornerRadius = UDim.new(0, 8)
            btnCorner.Parent = closeBtn
            
            -- Ø¥ÙŠÙ‚Ø§Ù Ø§Ù„Ù„Ø§Ø¹Ø¨ Ø¹Ù† Ø§Ù„Ø­Ø±ÙƒØ©
            if player.Character then
                local humanoid = player.Character:FindFirstChild("Humanoid")
                if humanoid then
                    humanoid.WalkSpeed = 0
                    humanoid.JumpPower = 0
                end
            end
        end
    end
end

