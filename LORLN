    if verifyBtn.Text == "VERIFY KEY" then
        verifyBtn.BackgroundColor3 = Color3.fromRGB(255, 80, 80)
    end
end)

verifyBtn.MouseLeave:Connect(function()
    if verifyBtn.Text == "VERIFY KEY" then
        verifyBtn.BackgroundColor3 = Theme.Accent
    end
end)

