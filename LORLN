        end
        
        task.delay(2, function()
            btn.Text = "EXECUTE"
            btn.BackgroundColor3 = Theme.Accent
        end)
    end)
end

-- ========== Ø¯Ø§Ù„Ø© ÙØªØ­ Ø§Ù„Ù‚Ø§Ø¦Ù…Ø© ==========
local function openMainWindow()
    if not main then return end
    
    if isWindowClosed then
        isWindowClosed = false
        main.Visible = true
        main.Size = UDim2.new(0, 0, 0, 0)
        main.Position = lastWindowPosition
        main.AnchorPoint = Vector2.new(0, 0)
        
        isMinimized = false
        
        if scroll then
            scroll.Visible = false
        end
        
        if minimizeBtnElement then
            minimizeBtnElement.Text = "-"
        end
        
        -- Ø£Ù†ÙŠÙ…ÙŠØ´Ù† Ø§Ù„ÙØªØ­
        local openTween = TS:Create(main, TweenInfo.new(0.35, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
            Size = originalSize
        })
        openTween:Play()
        
        openTween.Completed:Connect(function()
            if scroll then
                scroll.Visible = true
            end
        end)
    else
        if isMinimized then
            maximizeWindow()
        end
    end
end

-- ========== Ø¯Ø§Ù„Ø© ØªÙƒØ¨ÙŠØ± Ø§Ù„Ù†Ø§ÙØ°Ø© ==========
local function maximizeWindow()
    if not isMinimized or isAnimating or not main.Visible or isWindowClosed then return end
    
    isAnimating = true
    
    TS:Create(main, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
        Size = originalSize
    }):Play()
    
    task.wait(0.3)
    if scroll then scroll.Visible = true end
    if minimizeBtnElement then minimizeBtnElement.Text = "-" end
    isMinimized = false
    isAnimating = false
end

-- ========== Ø¯Ø§Ù„Ø© Ø¥Ù†Ø´Ø§Ø¡ Ù†Ø§ÙØ°Ø© Ù‚Ø§Ø¦Ù…Ø© Ø§Ù„Ù…Ø¨Ù†Ø¯ÙŠÙ† ==========
