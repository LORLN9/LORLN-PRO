local discordLink = "https://discord.gg/JECyp4y9gh"

local function copyToClipboard(text)
    if setclipboard then
        setclipboard(text)
        return true
    elseif writeclipboard then
        writeclipboard(text)
        return true
    end
    return false
end

Copy.MouseButton1Click:Connect(function()
    playSound(CLICK_SOUND)
    if copyToClipboard(discordLink) then
        Copy.Text = "âœ“ ØªÙ… Ø§Ù„Ù†Ø³Ø®"
        Copy.BackgroundColor3 = THEME.Success
        task.wait(1.5)
    end
    Copy.Text = "Ù†Ø³Ø® Ø§Ù„Ø±Ø§Ø¨Ø·"
    Copy.BackgroundColor3 = THEME.Accent
end)

--==============================
-- Lock Close Button 5 Seconds
--==============================
Close.Active = false
task.spawn(function()
    task.wait(5)
    Close.Active = true
end)

Close.MouseButton1Click:Connect(function()
    if not Close.Active then
        return -- Ù…Ù‚ÙÙ„ Ø­ØªÙ‰ Ø§Ù†ØªÙ‡Ø§Ø¡ 5 Ø«ÙˆØ§Ù†ÙŠ
    end
    playSound(CLICK_SOUND)
    TweenService:Create(Main, TweenInfo.new(0.35, Enum.EasingStyle.Back, Enum.EasingDirection.In), {
        Size = UDim2.new(0, 0, 0, 0),
        Position = UDim2.new(0.5, 0, 0.5, 0)
    }):Play()
    task.wait(0.35)
    ScreenGui:Destroy()
end)

--==============================
-- Drag System
--==============================
do
    local dragging, dragStart, startPos

    Header.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            dragStart = input.Position
            startPos = Main.Position
        end
    end)

    UserInputService.InputChanged:Connect(function(input)
        if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
            local delta = input.Position - dragStart
            Main.Position = UDim2.new(
                startPos.X.Scale, startPos.X.Offset + delta.X,
                startPos.Y.Scale, startPos.Y.Offset + delta.Y
            )
        end
    end)

    UserInputService.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = false
        end
    end)
end

--==============================
-- Intro Animation
--==============================
Main.Size = UDim2.new(0, 0, 0, 0)
TweenService:Create(Main, TweenInfo.new(0.6, Enum.EasingStyle.Back), {
    Size = UDim2.new(0, 500, 0, 290)
}):Play()

