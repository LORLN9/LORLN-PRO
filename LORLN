    autoCorner.CornerRadius = UDim.new(0, 8)
    autoCorner.Parent = autoBtn

    local autoStroke = Instance.new("UIStroke")
    autoStroke.Color = Color3.fromRGB(100, 0, 0)
    autoStroke.Thickness = 2
    autoStroke.Parent = autoBtn

    -- Ù†Øµ ØµØºÙŠØ± ØªØ­Øª Ø²Ø± Ø§Ù„Ø£ÙˆØªÙˆ
    local autoLabel = Instance.new("TextLabel")
    autoLabel.Size = UDim2.new(0, 35, 0, 15)
    autoLabel.Position = UDim2.new(1, -45, 0, 85)
    autoLabel.Text = "AUTO"
    autoLabel.TextColor3 = Theme.TextSecondary
    autoLabel.Font = Enum.Font.Gotham
    autoLabel.TextSize = 10
    autoLabel.BackgroundTransparency = 1
    autoLabel.Parent = card

    -- Ø§Ù„ØªØ­Ù‚Ù‚ Ø¥Ø°Ø§ ÙƒØ§Ù† Ù‡Ø°Ø§ Ø§Ù„Ø³ÙƒØ±Ø¨Øª Ù…ÙØ¹Ù„ Ø¨Ø§Ù„Ø£ÙˆØªÙˆ Ù…Ø³Ø¨Ù‚Ø§Ù‹
    if autoScripts[name] == true then
        autoBtn.ImageColor3 = Theme.Success
        autoStroke.Color = Theme.Success
        autoLabel.TextColor3 = Theme.Success
        
        -- ØªØ´ØºÙŠÙ„ Ø§Ù„Ø³ÙƒØ±Ø¨Øª ÙÙˆØ±Ø§Ù‹
        task.spawn(function()
