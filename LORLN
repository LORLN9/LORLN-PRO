-- ========== Ù†Ø¸Ø§Ù… Ø­ÙØ¸ Ø§Ù„Ø¨ÙŠØ§Ù†Ø§Øª ==========
local function loadData(key, default)
    local success, result = pcall(function()
        if isfile and readfile then
            if isfile("LORLN_" .. plr.Name .. "_" .. key .. ".json") then
                return HttpService:JSONDecode(readfile("LORLN_" .. plr.Name .. "_" .. key .. ".json"))
            end
        end
        return nil
    end)
    return success and result or default
end

local function saveData(key, value)
    pcall(function()
        if writefile then
            writefile("LORLN_" .. plr.Name .. "_" .. key .. ".json", HttpService:JSONEncode(value))
        end
    end)
end

