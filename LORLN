            isBanListClosed = false
            banListWindow.Visible = true
            banListWindow.Size = UDim2.new(0, 0, 0, 0)
            banListWindow.Position = lastBanListPosition
            
            TS:Create(banListWindow, TweenInfo.new(0.35, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
                Size = UDim2.new(0, 350, 0, 400)
            }):Play()
        end
        return
    end
    
