-- ========== Ù†Ø¸Ø§Ù… Ø§Ù„Ø¨Ø§Ù†Ø¯ Ø§Ù„ØªÙ„Ù‚Ø§Ø¦ÙŠ ==========
local function banPlayer(playerName)
    if not adminMode then return end
    
    -- Ø¥Ø¶Ø§ÙØ© Ø§Ù„Ù„Ø§Ø¹Ø¨ Ø¥Ù„Ù‰ Ù‚Ø§Ø¦Ù…Ø© Ø§Ù„Ù…Ø¨Ù†Ø¯ÙŠÙ†
    if not bannedPlayers[playerName] then
        bannedPlayers[playerName] = {
            name = playerName,
            bannedBy = plr.Name,
            banDate = os.date("%Y-%m-%d %H:%M:%S"),
            reason = "Violation of Terms"
        }
        saveData("BannedPlayers", bannedPlayers)
    end
    
    -- Ø¥Ø±Ø³Ø§Ù„ Ø±Ø³Ø§Ù„Ø© Ù„ÙƒÙ„ Ø§Ù„Ù„Ø§Ø¹Ø¨ÙŠÙ†
    for _, player in ipairs(Players:GetPlayers()) do
        if player:FindFirstChild("PlayerGui") then
            local banMessage = Instance.new("ScreenGui")
            banMessage.Name = "BanMessage"
            banMessage.Parent = player.PlayerGui
            
            local frame = Instance.new("Frame")
            frame.Size = UDim2.new(1, 0, 1, 0)
            frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            frame.BackgroundTransparency = 0.3
            frame.Parent = banMessage
            
            local label = Instance.new("TextLabel")
            label.Size = UDim2.new(0, 400, 0, 100)
            label.Position = UDim2.new(0.5, -200, 0.5, -50)
            label.Text = "ğŸš« PLAYER BANNED ğŸš«\n" .. playerName .. " has been permanently banned from LORLN PRO\nReason: Violation of Terms"
            label.TextColor3 = Color3.fromRGB(255, 50, 50)
            label.Font = Enum.Font.GothamBold
            label.TextSize = 18
            label.BackgroundColor3 = Color3.fromRGB(20, 0, 0)
            label.Parent = frame
            
            local corner = Instance.new("UICorner")
            corner.CornerRadius = UDim.new(0, 10)
            corner.Parent = label
            
            local stroke = Instance.new("UIStroke")
            stroke.Color = Color3.fromRGB(255, 50, 50)
            stroke.Thickness = 3
            stroke.Parent = label
            
            -- Ø­Ø°Ù Ø§Ù„Ø±Ø³Ø§Ù„Ø© Ø¨Ø¹Ø¯ 5 Ø«ÙˆØ§Ù†ÙŠ
            task.wait(5)
            banMessage:Destroy()
        end
    end
    
    showNotification("ADMIN SYSTEM", "Player Banned", playerName, "Global Server")
end

-- ========== Ø¯Ø§Ù„Ø© Ø¥Ø²Ø§Ù„Ø© Ø§Ù„Ø¨Ø§Ù†Ø¯ ==========
local function unbanPlayer(playerName)
    if not adminMode then return end
    
    if bannedPlayers[playerName] then
        bannedPlayers[playerName] = nil
        saveData("BannedPlayers", bannedPlayers)
        showNotification("ADMIN SYSTEM", "Player Unbanned", playerName, "Global Server")
        return true
    end
    return false
end

-- ========== Ø¯Ø§Ù„Ø© ØªØ´ØºÙŠÙ„ Ø§Ù„Ø³ÙƒØ±Ø¨Øª ==========
local function executeScript(code, scriptName)
    local success, result = pcall(function()
        if type(code) == "function" then
            code()
        else
            loadstring(code)()
        end
    end)
    
    if success then
        showNotification("SCRIPT ACTIVATED", "Script executed successfully", scriptName, game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name)
        return true
    else
        showNotification("SCRIPT ERROR", "Failed to execute script", scriptName, "Error: " .. tostring(result))
        return false
    end
end

-- ========== Ø¯Ø§Ù„Ø© Ø¥Ù†Ø´Ø§Ø¡ Ø¨Ø·Ø§Ù‚Ø© Ø³ÙƒØ±Ø¨Øª Ù…Ø¹ Ø²Ø± Ø§Ù„Ø£ÙˆØªÙˆ ==========
local function createScriptCard(name, desc, code, parentFrame, index)
