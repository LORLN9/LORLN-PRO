-- Ø¯Ø§Ù„Ø© Ø§Ù„ØªØ­ÙƒÙ… ÙÙŠ NightVision
local function toggleNightVision()
    local success, nightVision = pcall(function()
        return ReplicatedStorage:WaitForChild("HDAdminHDClient")
            :WaitForChild("Assets")
            :WaitForChild("NightVision")
    end)
    
    if success and nightVision then
        if nightVisionActive then
            -- Ø¥ÙŠÙ‚Ø§Ù Ø§Ù„Ù€ NightVision
            nightVision.Name = "NightVision"
            nightVisionActive = false
            print("NightVision Ø£ØµØ¨Ø­: NightVision âŒ")
        else
            -- ØªØ´ØºÙŠÙ„ Ø§Ù„Ù€ NightVision
            nightVision.Name = "1NightVision"
            nightVisionActive = true
            print("NightVision Ø£ØµØ¨Ø­: 1NightVision âœ…")
        end
    else
        print("âš ï¸ Ù„Ù… ÙŠØªÙ… Ø§Ù„Ø¹Ø«ÙˆØ± Ø¹Ù„Ù‰ NightVision")
    end
end

-- Ø¯Ø§Ù„Ø© Ø­Ø°Ù Ø§Ù„Ø¹Ù†Ø§ØµØ±
local function deleteElements()
    for _, name in ipairs(namesToDelete) do
        pcall(function()
            local target = playerGui:FindFirstChild(name, true)
            if target then
                target:Destroy()
                print("ğŸ—‘ï¸ Ø­Ø°Ù: " .. name)
            end
        end)
    end
    
    -- Ø§Ù„ØªØ­ÙƒÙ… ÙÙŠ NightVision
    toggleNightVision()
end

-- Ø¯Ø§Ù„Ø© Ø­Ø°Ù CmdBar ÙÙ‚Ø·
local function deleteCmdBars()
    pcall(function()
        for _, name in ipairs({"CustomTopBar", "CmdBar"}) do
            local target = playerGui:FindFirstChild(name, true)
            if target then
                target:Destroy()
                print("ğŸ—‘ï¸ Ø­Ø°Ù: " .. name)
            end
        end
    end)
end

-- Ø§Ù„Ø­Ù„Ù‚Ø© Ø§Ù„Ø±Ø¦ÙŠØ³ÙŠØ©
