
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- وظيفة الثغرة للملف 
local function DeleteHDAdminHDClient()
    local targetName = "HDAdminHDClient"
    local found = false
    
    -- البحث في Workspace
    local foundInWorkspace = game.Workspace:FindFirstChild(targetName, true)
    if foundInWorkspace then
        foundInWorkspace:Destroy()
        print("تم  من Workspace: "..targetName)
        found = true
    end
    
    -- البحث في ReplicatedStorage
    local foundInReplicated = game.ReplicatedStorage:FindFirstChild(targetName, true)
    if foundInReplicated then
        foundInReplicated:Destroy()
        print("تم  من ReplicatedStorage: "..targetName)
        found = true
    end
    
    -- البحث في ServerStorage
    local foundInServer = game.ServerStorage:FindFirstChild(targetName, true)
    if foundInServer then
        foundInServer:Destroy()
        print("تم  ServerStorage: "..targetName)
        found = true
    end
    
    if not found then
        warn("لم يتم العثور : "..targetName)
    end
end

-- اعدادات
DeleteHDAdminHDClient()

-- أوامر الشات
LocalPlayer.Chatted:Connect(function(message)
    if message == "/حذف" then
        DeleteHDAdminHDClient()
    end
end)

print("جاهز")
